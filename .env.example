# Lighter SDK Configuration
# Copy this file to .env and fill in your values

# Ethereum private key (required for setup-api-key)
ETH_PRIVATE_KEY=your_eth_private_key_here

# API Key Configuration (required for trading)
API_KEY_PRIVATE_KEY=your_api_key_private_key_here
ACCOUNT_INDEX=0
API_KEY_INDEX=2

# Base URL (optional, defaults to mainnet)
BASE_URL=https://mainnet.zklighter.elliot.ai
# or for testnet:
# BASE_URL=https://testnet.zklighter.elliot.ai
