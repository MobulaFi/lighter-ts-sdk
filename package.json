{
  "name": "zklighter-sdk",
  "version": "1.2.0",
  "description": "TypeScript SDK for interacting with the Lighter (zkLighter) API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "key": "ts-node -r dotenv/config examples/scripts/generate-api-key.ts",
    "order": "ts-node -r dotenv/config examples/scripts/create-order.ts",
    "cancel-order": "ts-node -r dotenv/config examples/scripts/cancel-order.ts",
    "info": "ts-node -r dotenv/config examples/scripts/get-info.ts",
    "auth-token": "ts-node -r dotenv/config examples/scripts/auth-token.ts",
    "get-auth-token": "ts-node -r dotenv/config examples/scripts/get-auth-token.ts",
    "fast-withdraw": "ts-node -r dotenv/config examples/scripts/fast-withdraw.ts",
    "account": "ts-node -r dotenv/config examples/scripts/check-account.ts",
    "positions": "ts-node -r dotenv/config examples/scripts/positions-interaction.ts",
    "leverage": "ts-node -r dotenv/config examples/scripts/update-leverage.ts",
    "market": "ts-node -r dotenv/config examples/scripts/market-details.ts",
    "build": "tsc && rm -rf dist/signers && cp -r src/signers dist/signers",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "lighter",
    "zklighter",
    "sdk",
    "typescript",
    "trading",
    "defi",
    "api",
    "ethereum"
  ],
  "author": "",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/MobulaFi/lighter-ts-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/MobulaFi/lighter-ts-sdk/issues"
  },
  "homepage": "https://github.com/MobulaFi/lighter-ts-sdk#readme",
  "dependencies": {
    "axios": "^1.6.0",
    "dotenv": "^16.0.0",
    "ethers": "^6.0.0",
    "koffi": "^2.14.1"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  }
}
